{
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "pretty": true,
    "newLine": "lf",
    "stripInternal": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
